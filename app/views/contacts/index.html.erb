<h1>お問い合わせ</h1>
<%= form_for @contact do |f| %>
<ul>
  <p>名前</p>
<li><%= f.text_field :name %></li>
  <p>フリガナ</p>
<li><%= f.text_field :name2 %></li>
  <p>メールアドレス</p>
<li><%= f.text_field :addres %></li>
  <p>内容</p>
<li><%= f.text_area :content %></li>
<li><%= f.submit %>
</ul>
<% end %>
<%= link_to "Top", root_path %>